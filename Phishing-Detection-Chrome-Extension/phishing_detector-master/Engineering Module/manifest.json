{
  "manifest_version": 3,
  "name": "Web Shield Pro",
  "version": "0.1",
  "icons": {
    "16": "detective.png",
    "48": "detective.png",
    "128": "detective.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "permissions": [
    "notifications"
  ]
}
